<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tennis_liga">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tennis_liga">
        <activity
            android:name=".Prueba"
            android:exported="false"
            android:theme="@style/Theme.Liga_padel"/>
        <activity
            android:name=".CompetenciasActivity"
            android:exported="false" />
        <activity
            android:name=".Estadisticas"
            android:exported="false"
            android:theme="@style/Theme.Liga_padel"/>
        <activity
            android:name=".Login"
            android:exported="false"
            android:theme="@style/Theme.Liga_adminX" />
        <activity
            android:name=".Administrador"
            android:exported="false"
            android:theme="@style/Theme.Liga_admin" />
        <activity
            android:name=".Home"
            android:exported="false"
            android:theme="@style/Theme.Liga_padelX" />
        <activity
            android:name=".NuevoUsuario"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.Liga_padelX">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>